  "scripts": {
    "seed": "npx -y tsx libs/database/prisma/seed.ts",
    "db:generate": "npx prisma generate --schema=libs/database/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema=libs/database/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=libs/database/prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema=libs/database/prisma/schema.prisma",
    "start": "npx nx run-many --target=serve --output-style=stream-without-prefixes",
    "be": "npx nx run-many --target=serve --projects=tag:type:be --output-style=stream-without-prefixes",
    "fe": "npx nx run-many --target=serve --projects=tag:type:fe --output-style=stream-without-prefixes",
    "build": "nx run-many --target=build --all"
  },
  "private": true,
